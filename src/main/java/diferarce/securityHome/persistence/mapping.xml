<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.example.BlogMapper">

	<select id="getMicroControllers" resultType="MicroController">
		SELECT name,port FROM microControllers
	</select>
	
		
	<resultMap  type="diferarce.securityHome.dao.A" id="tutorialResultWithAutor">
		<id column="task" property="id" javaType="int"/>
	    <result column="name" property="name" javaType="string"/>	    
	    <collection property="deviceId" javaType="ArrayList" ofType="diferarce.securityHome.dao.B" column="deviceId" />
	    
	    
	</resultMap>
	

	<select id="ejemplo"  resultMap="tutorialResultWithAutor">
		select a.id as task, a.name, b.deviceId
		from task a  INNER JOIN scheduledTask b 	ON	 a.id = b.taskId  
		
	</select>
		
</mapper>